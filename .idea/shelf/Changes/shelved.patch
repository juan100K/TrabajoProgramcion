+package uq.trabajoprogramacion.model;
+import uq.trabajoprogramacion.controller.*;
+
+import java.util.Objects;
+
+public class Deportes {
+
+    private  String deportes;
+    private String descripcion;
+
+    public Deportes(String deportes, String descripcion) {
+        this.deportes = deportes;
+        this.descripcion = descripcion;
+    }
+
+    public String getDeportes() {
+        return deportes;
+    }
+
+    public void setDeportes(String deportes) {
+        this.deportes = deportes;
+    }
+
+    public String getDescripcion() {
+        return descripcion;
+    }
+
+    public void setDescripcion(String descripcion) {
+        this.descripcion = descripcion;
+    }
+
+    @Override
+    public boolean equals(Object object) {
+        if (this == object) return true;
+        if (object == null || getClass() != object.getClass()) return false;
+        Deportes deportes1 = (Deportes) object;
+        return Objects.equals(deportes, deportes1.deportes) && Objects.equals(descripcion, deportes1.descripcion);
+    }
+
+    @Override
+    public int hashCode() {
+        return Objects.hash(deportes, descripcion);
+    }
+}
